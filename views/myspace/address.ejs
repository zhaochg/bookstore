<!--地址-->
<div style="margin-left: 10px;margin-right: 10px">
    <div class="row">
        <br>
        <div class="text-danger text-left">收货地址</div>
        <br>
        <br>
        <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#articleModal">
            新增地址
        </button>
        <br>
        <br>
        <table class="table" >
            <thead>
            <tr>
                <th>收货人</th>
                <th>电话号码</th>
                <th>详细地址</th>
                <th>设置默认地址</th>
                <th>操作</th>
                <th></th>
            </tr>
            </thead>
            <tbody id="categoryList">
                <tr class="td" >
                    <td>于静</td>
                    <td>河南省郑州市</td>
                    <td>郑州工商学院7号楼</td>
                    <td>452470</td>
                    <td>15638287287</td>
                    <td>修改|删除</td>
                    <td>
                        <button type="button" class="btn btn-warning">
                            默认地址
                        </button>
                    </td>
                </tr>
            </tbody>
        </table>
        <!--新增地址模态框-->
        <div class="modal fade" id="articleModal" tabindex="-1">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <input id="id" type="hidden">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">新增地址</h4>
                    </div>
                    <div class="modal-body">
                            <div class="form-group">
                                收货人姓名<font color="red">*</font>
                                <input type="text" class="form-control" id="name">
                            </div>
                            <div class="form-group">
                                手机号<font color="red">*</font>
                                <input type="text" class="form-control" id="phone01">
                            </div>
                            <div class="form-group">
                                收货地址<font color="red">*</font>
                                <input type="text" class="form-control" id="address01">
                            </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-warning" data-dismiss="modal"  onclick="saveAddressList()">保存</button>
                    </div>
                </div>
            </div>
        </div>
        <!--页脚-->
    </div>
</div>